{
  "name": "helper-uigradient",
  "description": "Template helper that generates the CSS for any of the gradients from Ghosh/uiGradients",
  "version": "1.0.0",
  "homepage": "https://github.com/helpers/helper-uigradient",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/helper-uigradient",
  "bugs": {
    "url": "https://github.com/helpers/helper-uigradient/issues"
  },
  "license": "MIT",
  "files": [
    "gradients.json",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": "0.10.0"
  },
  "scripts": {
    "test": "mocha",
    "build": "node build"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "extend-shallow": "^2.0.1"
  },
  "devDependencies": {
    "delete": "^1.0.1",
    "gh-clone": "^0.5.0",
    "glob": "^7.1.2",
    "gulp-format-md": "^0.1.12",
    "handlebars": "^4.0.10",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "compile",
    "css",
    "generate",
    "gradient",
    "helper",
    "helpers",
    "render",
    "template",
    "templates",
    "ui",
    "uigradient"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "handlebars-helpers",
        "helper-date",
        "helper-html-table",
        "helper-octicon",
        "template-helpers"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "lintDeps": {
    "devDependencies": {
      "files": {
        "patterns": [
          "build.js"
        ]
      }
    }
  }
}
